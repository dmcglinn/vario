## The function is currently defined as
function (x) 
{
    S <- ncol(x)
    N <- nrow(x)
    Psp <- array(0, dim = c(S, sqrt(N), sqrt(N)))
    icount <- 0
    for (col in 1:sqrt(N)) {
        for (row in 1:sqrt(N)) {
            icount <- icount + 1
            for (sp in 1:S) {
                if (x[icount, sp] > 0) {
                  Psp[sp, row, col] <- 1
                }
            }
        }
    }
    Psp
  }
}
